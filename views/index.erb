<!-- Our Work Section -->
<div id="work" class="page">
	<div class="container">
    	<!-- Title Page -->
        <div class="row">
            <div class="span12">
                <div class="title-page">
                    <h2 class="title">My Projects</h2>
                    <h3 class="title-description">Check Out My Projects In Full Detail on <a href="https://github.com/naysayer">Github</a>.</h3>
                    
                    <div class="page-description">
                		<p>
                            Below you'll find a collection of my work from different categories ranging from Ruby on Rails to iOS. Click on any of projects to gain more information, as well as view a live demo. You can view any of these project's code on my github account, <a href="https://github.com/naysayer" target="_blank">here</a>. Feel free to <a href="#contact">contact me</a> with any questions you may have. 
                        </p>
                	</div>
                
                </div>    
            </div>
        </div>
        <!-- End Title Page -->
        
        <!-- Portfolio Projects -->
        <div class="row">
        	<div class="span12">
            	<!-- Filter -->
                <nav id="options" class="work-nav">
                    <ul id="filters" class="option-set" data-option-key="filter">
                    	<li class="type-work">Type of Project</li>
                        <li><a href="#filter" data-option-value="*" class="selected">All Projects</a></li>
                        <li><a href="#filter" data-option-value=".ruby">Ruby on Rails</a></li>
                        <li><a href="#filter" data-option-value=".wordpress">WordPress</a></li>
                        <li><a href="#filter" data-option-value=".javascript">JavaScript</a></li>
                        <li><a href="#filter" data-option-value=".video"></a></li>
                    </ul>
                </nav>
                <!-- End Filter -->
            </div>
            
            <div class="span12">
            	<div class="row">
                	<section id="projects">
                    	<ul id="thumbs">
                        
                            <% @projects.each do |project| %>
							<!-- Item Project and Filter Name -->
                        	<%= "<li class='item-thumbs span3 #{project.category}'>" %>
                                <!-- Fancybox - Gallery Enabled - Title - Full Image -->
                                <%= project.display_name %>
                                <%= "<a class='hover-wrap fancybox' data-fancybox-group='gallery' title=#{project.display_name} href='images/#{project.name}_large.png'>" %>
                                    <span class="overlay-img"></span>
                                    <span class="overlay-img-thumb font-icon-search"></span>
                                </a>
                                <!-- Thumb Image and Description -->
                                <%= "<img src='images/#{project.name}_thumb.png'/>"%>
                                <div class="desc hidden"><%= project.description.to_s %></div>
                                <div class="project_title hidden"><%= project.display_name %></div>
                            </li>
                        	<!-- End Item Project -->
                            <% end %>

                        </ul>
                    </section>
                    
            	</div>
            </div>
        </div>
        <!-- End Portfolio Projects -->
    </div>
</div>
<!-- End Our Work Section -->

